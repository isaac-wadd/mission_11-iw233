
@{}

@model IEnumerable<string>

@foreach (string c in Model) {
    <li class="nav-item">
        <a class="nav-link text-light" asp-controller="Home" asp-action="Index" asp-route-bookCategory="@c" asp-route-pageNum="1">
            <button class="btn@(ViewBag.selectedCategory == c ? " btn-primary" : " btn-secondary" )">@c</button>
        </a>
    </li>
}
